<%- include ('./partials/header') %>

<%- include ('./partials/navbar') %>

<%- include ('./partials/left') %>

<div class="container">
    <br><br><br><br>
    <h1>Device ID: <%= id %></h1>
    <hr>
  </div>
    <div class="container">
      <div class="row justify-content-start">
        <div class="col-6">

          <h1>Humidity</h1>
    <table class="table">
        <thead>
          <tr>
            <th scope="col">Date</th>
            <th scope="col">Humidity (%)</th>
          </tr>
        </thead>
        <tbody>
          <% dataHumidity.forEach(row => { %>
          <tr>
            <% row.forEach(element => { %>
            <td>
              <%= element %>
            </td>
            <% }) %>
          </tr>
          <% }) %>
        </tbody>
      </table>

        </div>
        <div class="col-6">

          <h1>Temperature:</h1>
    <table class="table">
      <thead>
        <tr>
          <th scope="col">Date</th>
          <th scope="col">Temperature (%)</th>
        </tr>
      </thead>
      <tbody>
        <% dataTemperature.forEach(row => { %>
        <tr>
          <% row.forEach(element => { %>
          <td>
            <%= element %>
          </td>
          <% }) %>
        </tr>
        <% }) %>
      </tbody>
    </table>

        </div>
      </div>
    </div>




  

    
      



